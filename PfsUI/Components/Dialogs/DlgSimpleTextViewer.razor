@namespace PfsUI.Components

<MudDialog>
    <TitleContent>
        <b>@Title</b>
    </TitleContent>
    <DialogContent>

        <MudTextField T="string"  Text="@Text" Variant="Variant.Outlined" ReadOnly="true" Lines="10" />

    </DialogContent>

    <DialogActions>
        <MudButton OnClick="DlgCancel">OK</MudButton>
    </DialogActions>
</MudDialog>
